
# Variables de entorno para HoneyLabs
# Copia este archivo como .env y completa los valores requeridos

# URL para Prisma Accelerate
# Ejemplo:
# DATABASE_URL="prisma+postgres://accelerate.prisma-data.net/?api_key=YOUR_API_KEY"
DATABASE_URL=
# Conexión directa para migraciones
DIRECT_DB_URL=
PRISMA_DATA_PROXY=true
# Clave secreta para firmas JWT
JWT_SECRET=

# Ejemplos de otras variables (correos SMTP)
EMAIL_ADMIN=
SMTP_USER=
SMTP_PASS=
EMAIL_DESTINO_ESTANDAR=
EMAIL_DESTINO_VALIDACION=

# Dominio raíz para compartir cookies entre subdominios en producción
COOKIE_DOMAIN=

# Prefijo opcional para rutas cuando despliegas en un subdirectorio
NEXT_PUBLIC_BASE_PATH=

# URL del webhook para enviar notificaciones de chat a Slack
SLACK_WEBHOOK_URL=
NEXT_PUBLIC_WS_URL=
# Parámetros para build móvil
GITHUB_TOKEN=
GITHUB_REPO=

